[ascender]
ascender_host ansible_host=x.x.x.x ansible_user=username ansible_ssh_pass=password ansible_ssh_private_key_file=ssh_key.pem

[ledger]
ledger_host ansible_host=x.x.x.x ansible_user=username ansible_ssh_pass=password ansible_ssh_private_key_file=ssh_key.pem

[ascender:vars]
ASCENDER_NAMESPACE=ascender
ASCENDER_VERSION=22.5.0
ANSIBLE_OPERATOR_VERSION=2.4.0

[ledger:vars]
LEDGER_VERSION=latest
LEDGER_NAMESPACE=ledger

# Set default passwords (can not currently change passwords after install)
LEDGER_PASSWORD=
LEDGER_DB_PASSWORD=

# Node Port
# Uncomment to set a specific port, otherwise it will be random within the proper range
